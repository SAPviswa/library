<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout">
    <Dialog id="signUpDialog" title="Sign Up" class="signuphead">
        <HBox id="_IDGenHb" justifyContent="Center">
            <VBox id="vBoxSignUp">
            <Label id="_IDGenLabel4" text="" />
                <Label id="_IDGenLabel1" text="User Name" />
                <Input id="firstNameInput" placeholder="Enter User Name" class="input"/>
                <Label id="_IDGenLabelLname" text="Password Name" />
                <Input id="lastNameInput" placeholder="Enter Password Name" class="input" type="Password"/>
                <Label id="_IDGenLabel3" text="Email" />
                <Input id="FullnameInput" type="Text" placeholder="Enter Fullname" class="input"/>
                <Label id="_IDGenLabel04" text="Address" />
                <Input id="emailInput" type="Text" placeholder="Enter Address" class="input"/>
                <Label id="_IDGenLabel05" text="Phone no" />
                <Input id="PhnoInput" type="Text" placeholder="Enter Phone no" class="input"/>
                <Label id="_IDGenLabel06" text="Role" />
                <Input id="RoleInput" type="Text" placeholder="Enter Role" class="input"/>
                <Label id="_IDGenLabel5" text="" />
            </VBox>
        </HBox>
        <beginButton>
            <Button id="_IDGenButton1" text="Submit" press="onSubmitSignUp" />
        </beginButton>
        <endButton>
            <Button id="_IDGenButton2" text="Cancel" press="onCancelSignUp" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
